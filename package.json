{
  "name": "nextjs-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "watch-sass": "sass --watch sass/main.scss css/style.css",
    "compile-sass": "sass sass/main.scss css/style.css",
    "prefix-css": "postcss --use autoprefixer -b 'last 2 versions' css/style.css -o css/index.css",
    "build-css": "npm-run-all compile-sass prefix-css"
  },
  "dependencies": {
    "next": "13.4.4",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^10.1.0",
    "sass": "^1.62.1",
    "autoprefixer": "^10.4.14"
  }
}
